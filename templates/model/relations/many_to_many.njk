{{ relation.relationName }}: {
        relation: Model.ManyToManyRelation,
        modelClass: {{ relation.relationClass }},
        join: {
          from: '{{ relation.parentTable }}.{{ relation.parentTableColumn }}',
          through: {
            // persons_movies is the join table.
            from: '{{ relation.midTable }}.{{ relationMidTableFromColumn}}',
            to: '{{ relation.midTable }}.{{ relationMidTableToColumn}}'
          },
          to: '{{ relation.relationEndTable }}.{{ relation.endTableColumn }}'
        }
      }